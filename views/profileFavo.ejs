<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Profile</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <link rel="stylesheet" type="text/css" href="/css/main.css">
  </head>
  <body>
    <!-- Example -->
    <div class='userlogout'>
    <form action="/auth?_method=DELETE" method="post">
      <input type="submit" value="Logout">
    </form>
    </div>

  <form class ="searchform" action="/search/searchIcon" method="GET">
    <input type="text"  name="icon"  placeholder="Enter Search">
    <input type="submit" value="Search">
  </form>

 <!-- <div class="container"> -->
  <h1 id="myFavTitle"> FAVORITES </h1>
  <div class='container2'>
  <% favorites.forEach(function(fav) { %>
      <div class="result">
      <h4>Icon: <%= fav.id %></h4>
      <img class="favoriteItems" src="<%= fav.previewURL%>" alt="icon" />
      <form action="/favorites/deleteFavorite/<%=fav._id %>?_method=DELETE" method="post">
       <input class="middleTag" type="submit" value="Delete">
      </form>
      </div>
<% }) %>
</div>



</body>
</html>
